<Page
    x:Class="VješaloXD.singleplayer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:VješaloXD"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <TextBlock x:Name="naslov" HorizontalAlignment="Left" Margin="820,52,0,0" Text="Levels" TextWrapping="Wrap" VerticalAlignment="Top" Height="161" Width="349" FontSize="128px" FontFamily="Bradley Hand ITC" FontWeight="Normal"/>
        <Button x:Name="gumbLevel1" CornerRadius="10" Foreground="white" Background="black" Content="Level 1" Margin="310,300,0,0" VerticalAlignment="Top" Height="100" Width="205" FontSize="60" Click="gumbLevel1_Click" FontFamily="Bradley Hand ITC"/>
        <Button x:Name="gumbLevel2" CornerRadius="10" Foreground="white" Background="black" Content="Level 2" Margin="600,300,0,0" VerticalAlignment="Top" Height="100" Width="205" FontSize="52px" Click="gumbLevel2_Click" FontFamily="Bradley Hand ITC"/>
        <Button x:Name="gumbLevel3" CornerRadius="10" Foreground="white" Background="black" Content="Level 3" Margin="890,300,0,0" VerticalAlignment="Top" Height="100" Width="205" FontSize="52px" Click="gumbLevel3_Click" FontFamily="Bradley Hand ITC"/>
        <Button x:Name="gumbLevel4" CornerRadius="10" Foreground="white" Background="black" Content="Level 4" Margin="1180,300,0,0" VerticalAlignment="Top" Height="100" Width="205" FontSize="52px" Click="gumbLevel4_Click" FontFamily="Bradley Hand ITC"/>
        <Button x:Name="gumbLevel5" CornerRadius="10" Foreground="white" Background="black" Content="Level 5" Margin="1470,300,0,0" VerticalAlignment="Top" Height="100" Width="205" FontSize="52px" RenderTransformOrigin="-0.172,0.386" Click="gumbLevel5_Click" FontFamily="Bradley Hand ITC"/>
        <Button x:Name="gumbLevel6" CornerRadius="10" Foreground="white" Background="black" Content="Level 6" Margin="310,420,0,0" Height="100" Width="205" FontSize="52px" VerticalAlignment="Top" Click="gumbLevel6_Click" FontFamily="Bradley Hand ITC" />
